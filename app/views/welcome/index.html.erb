<h1>Hello World</h1>
<p>This was travis! Part 2</p>

<%= link_to "Create Trip", new_trip_path %><br />

<% unless current_user.nil? %>
	<%= link_to "Sign Out", sign_out_path %><br />
<% else %>
	<%= link_to "Sign Up", new_account_path %><br />
	<%= link_to "Sign In", sign_in_path %><br />
<% end %>

<% ActsAsTaggableOn::Tag.most_used.each do |x|%>
    <%= x %><br />
<% end %>

<% unless current_user.nil? %>
	<p>My trips:</p>
	<% @trips.each do |trip| %>
		<% if  trip.account_id == current_user.id%>
			<%= link_to trip.title, trip %>
		<% end %>
	<% end %>
<% end %>

<p>All trips:</p>
<% @trips.each do |trip| %>
	<%= link_to trip.title, trip %>
<% end %>